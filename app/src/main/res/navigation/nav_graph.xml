<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/alerts_fragment">

    <action
        android:id="@+id/action_go_to_settings"
        app:destination="@id/settings_fragment"
        />

    <fragment
        android:id="@+id/alerts_fragment"
        android:name="com.example.friendkeepr.AlertsFragment"
        tools:layout="@layout/fragment_alerts"
        android:label="Alerts"
        />

    <fragment
        android:id="@+id/friends_fragment"
        android:name="com.example.friendkeepr.FriendsFragment"
        tools:layout="@layout/fragment_friends"
        android:label="Friends"
        />

    <fragment
        android:id="@+id/groups_fragment"
        android:name="com.example.friendkeepr.CategoriesFragment"
        tools:layout="@layout/fragment_categories"
        android:label="Groups"
        />

    <fragment
        android:id="@+id/import_fragment"
        android:name="com.example.friendkeepr.ImportFragment"
        tools:layout="@layout/fragment_import"
        android:label="Import"
        />

    <fragment
        android:id="@+id/settings_fragment"
        android:name="com.example.friendkeepr.SettingsFragment"
        android:label="Settings"
        />

</navigation>